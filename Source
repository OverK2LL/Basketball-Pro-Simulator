	getgenv().Clicker = v
	while true do
		if not getgenv().Clicker then return end
		    local A_1 = "Shoot"
            local A_2 = "Green"
            local Event = game:GetService("ReplicatedStorage").RF
            Event:InvokeServer(A_1, A_2)
			wait(3)

        end
    end)

    MainSection:NewToggle("Auto Rebirth", "Rebirths", function(v)
        getgenv().autohit = v
        while true do
            if not getgenv().autohit then return end
            local A_1 = "Rebirth"
            local Event = game:GetService("ReplicatedStorage").RF
            Event:InvokeServer(A_1)
            
game:GetService("ReplicatedStorage").RF:InvokeServer(unpack(args))
wait(1)
        end
    end)

